{
  "compilerOptions": {
    // "target": "es2016",
    // "module": "ES2020",
    "rootDir": "./src",
    // "moduleResolution": "Node",
    // "resolveJsonModule": true,
    "outDir": "./dist",
    // "esModuleInterop": true,
    // "forceConsistentCasingInFileNames": true,
    "strict": true,
    // "skipLibCheck": true

    /* Copied from:  https://github.com/jupyterlab/jupyterlab/blob/main/tsconfigbase.json */
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "esModuleInterop": true,
    "incremental": true,
    "module": "esnext",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "ES2018",
    "types": []
  },
  "include": ["./src"]
}
